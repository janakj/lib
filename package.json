{
    "name": "@janakj/lib",
    "version": "1.0.1",
    "description": "A library of common TypeScript functions",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "rm -rf build && tsc -p tsconfig.esm.json && tsc -p tsconfig.cjs.json && echo '{\"type\": \"commonjs\"}' > build/cjs/package.json"
    },
    "author": "Jan Janak <jan@janakj.org>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/janakj/lib.git"
    },
    "devDependencies": {
        "@types/base64-js": "^1.3.0",
        "@types/express": "^4.17.13",
        "@types/node": "^16.11.6",
        "@types/node-fetch": "^3.0.3",
        "@types/urijs": "^1.19.17",
        "@types/uuid": "^8.3.1",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "eslint": "^8.0.1",
        "typescript": "^4.4.4"
    },
    "peerDependencies": {
        "base64-js": "^1.5.1",
        "express": "^4.17.1",
        "urijs": "^1.19.7",
        "uuid": "^8.3.2"
    },
    "exports": {
        "./*": {
            "require": "./build/cjs/*.js",
            "import": "./build/esm/*.js"
        }
    }
}
